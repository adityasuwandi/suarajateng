<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="50dp"
        android:bottom="50dp">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/colorTimelineNonActive"/>
            <size
                android:width="160dp"
                android:height="150dp" />
        </shape>
    </item>
    <item
        android:left="60dp"
        android:right="60dp">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/colorTimelineNonActive"/>
            <size
                android:width="15dp"
                android:height="100dp"/>
        </shape>
    </item>
</layer-list>
